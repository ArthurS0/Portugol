#include <stdio.h>
 int main() {
 float nota;
 for (;;) { 
printf("Digite uma nota de 0 a 10: ");
 scanf("%f", &nota); 
if (nota < 0 || nota > 10) {
 printf("Valor inv√°lido! Por favor, digite uma nota entre 0 e 10.\n");
 } else {
 break; 
} 
} printf("A nota digitada foi: %1.f\n", nota);
 return 0;
 }
